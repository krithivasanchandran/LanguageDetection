jQuery(document).ready(function($) { 
   $("#resumeupload").change(function () 
   { 
     var iSize = ($("#resumeupload")[0].files[0].size / 1024); 
     var isizemb = ($("#resumeupload")[0].files[0].size / 1024 / 1024);
     if (iSize / 1024 > 6.5) { 
        $("input[type=submit]").attr('disabled','disabled');
        alert('Your file is larger than 6mb. Please choose a smaller file. Your file is ' + isizemb.toFixed(2) + 'mb');
        $("#resumeupload").val("");
        $('#submitjob').attr('disabled','disabled');
     }
     
         var ext = $('#resumeupload').val().split('.').pop().toLowerCase();
    if (iSize / 1024 < 6.5){
     if($.inArray(ext, ['pdf']) == -1) {
    		alert('Invalid extension! Please upload a PDF copy of your resume. See the directions below or install a PDF printer to save any file as a PDF');
    		$('#submitjob').attr('disabled','disabled'); 
    	}}
    	
      if (iSize / 1024 < 6 && $.inArray(ext, ['pdf']) == 1 && $('#dayswork').val() != '' && $('textarea#hourstowork').val() != '' && $('#currentlyInSchool').val() != '' && $('#howdidyouhear').val() != '' && $('#fullname').val() != '' && $('#ifUnder18').val() != '' && $('#everAppliedHereB4').val() != '' && $('#legallyAllowedToWork').val() != '' && $('#typeOfEmployDesired').val() != '' && $('#convicted').val() != '' &&  $('#address').val() != '' && $('#city').val() != '' && $('#state').val() != '' && $('#zipcode').val() != '' && $('#phone').val() != '' && $('#email').val() != '' && $('#date8').val() != '' && $('#salaryreq').val() != '' && $('#position').val() != '') {
      	$('#submitjob').removeAttr('disabled');
      } else if ($('#resumeupload').val == '' && $('#dayswork').val() != '' && $('textarea#hourstowork').val() != '' && $('#currentlyInSchool').val() != '' && $('#howdidyouhear').val() != '' && $('#fullname').val() != '' && $('#ifUnder18').val() != '' && $('#everAppliedHereB4').val() != '' && $('#legallyAllowedToWork').val() != '' && $('#typeOfEmployDesired').val() != '' && $('#convicted').val() != '' &&  $('#address').val() != '' && $('#city').val() != '' && $('#state').val() != '' && $('#zipcode').val() != '' && $('#phone').val() != '' && $('#email').val() != '' && $('#date8').val() != '' && $('#salaryreq').val() != '' && $('#position').val() != ''){
      	$('#submitjob').removeAttr('disabled');
      }
	
 	});
    });

</script>
