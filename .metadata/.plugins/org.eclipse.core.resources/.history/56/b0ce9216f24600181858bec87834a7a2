jQuery(document).ready(function($) { 
var selection = ["no"];
$('#ifunder18div').hide();
$('#ifUnder18').change(function () {
    if ($.inArray(this.value, selection) >= 0) {
        $('#ifunder18div').hide();
    } else {
        $('#ifunder18div').hide();
    }
});
});

