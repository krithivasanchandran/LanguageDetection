				if(ptr.matcher(body).matches()){
					System.out.println(" Phone number match found  :::::::::: " + ptr.matcher(body).group());
				}
				linkedlist.remove(sr);
			}
		}
		
		System.out.println(" linkedlist final size  " + linkedlist.size());
		}
	}
